                    display flex
                    p
                        padding 0
                        padding-left 5px
                        margin 0
                        margin-bottom 15px
                        font-size .9rem
                        line-height 1.3
                        text-align justify
                        font-family font-light
            .form-group, .select-group
                margin-bottom 15px
            label
                color #286b94
                font-size 1rem
            input, select
                border 1px solid #286b94
                color #286b94
                background-color transparent
                font-size 1rem
            .button_
                text-align center
                .submit_button
                    background-color #286b94
                    color #f4f4f4
                    border 0
                    padding 4px 25px 3px
                    border-radius 6px
                    font-family font-bold
                    font-size 1rem

