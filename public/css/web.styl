@font-face
    font-family font-light
    font-style normal
    src url(../fonts/IBMPlexSans-Light.ttf)

@font-face
    font-family font-medium
    font-style normal
    src url(../fonts/IBMPlexSans-Medium.ttf)

@font-face
    font-family font-bold
    font-style normal
    src url(../fonts/IBMPlexSans-Bold.ttf)

@font-face
    font-family font-semibold
    font-style normal
    src url(../fonts/IBMPlexSans-SemiBold.ttf)

@font-face
    font-family font-mediumitalic
    font-style normal
    src url(../fonts/IBMPlexSans-MediumItalic.ttf)

@font-face
    font-family font-regular
    font-style normal
    src url(../fonts/IBMPlexSans-Regular.ttf)

@font-face
    font-family font-regular2
    font-style normal
    src url(../fonts/IBMPlexMono-Regular.ttf)

body
    background #FFF
    margin 0px
    font-family font-regular
    overflow-x hidden
    color #000

section
    position relative

.btn, input, select, textarea
    &:focus
        box-shadow none !important
form
    display contents

strong
    font-family font-bold

.gracias
    padding 200px 0 70px
    .title
        text-align center
        h3
            font-family font-bold
            font-size 2.8rem
            color #00ac67
        p
            font-size 1.2rem
            color #000
            font-family font-light
            line-height 1.3

#modal1, #modal2, #modal3, #modal4
    .modal-header
        padding 9px 1rem
        border-bottom 0
    .modal-body
        display flex
        justify-content space-around
        a
            text-decoration none
            font-size 1rem
            font-family font-bold
            color #00f
        i
            cursor pointer
        strong
            color #00f

#copy-text
    margin 27px 0 19px
    display block
    font-size .9rem
    color #161616
    line-height 1.3
    border 2px solid #161616
    border-radius 15px
    padding 20px
    span
        font-family font-medium
        color #0f62fe
.btn-copy
    font-size .9rem
    background-color #0f62fe
    padding 3px 25px
    color #FFF
    border-radius 0
    &:hover
        color #FFF

.replay
    background-color #e9ecef
    height 100vh
    .modal-body
        padding-top 10vh
        border 0
        .formulario
            background-color #f4f4f4
            border-radius 5px
            h1
                padding 20px 14px 0
                color #286b94
                font-size 1.5rem
                font-family font-bold
                text-align center
                margin-bottom 20px
            p
                color #286b94
                text-align center
                padding 0 14px 20px
                margin 0
                font-size 1rem
                font-family font-light
        form
            padding 20px 14px
            margin 0
            border-radius 5px
            background-color #f4f4f4
            box-shadow 0 0.5rem 1rem rgba(0, 0, 0, .15) !important
            .form-check
                padding-left calc(var(--bs-gutter-x) * .5)
                .form-check-input
                    margin-left 0
                    &:checked
